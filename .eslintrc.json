{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "quotes": [
        "error",
        "double"
    ],
    "semi": ["error", "always"],
    "no-trailing-spaces": "error",
    "indent": ["error", 2]


  }
}
